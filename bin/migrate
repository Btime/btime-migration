#!/usr/bin/env node

const { migrate } = require('./../src/migrate/migrate')
const chalk = require('chalk')

migrate()
  .then(resume)
  .catch(err => console.log(chalk.bold.red(err)))

function resume (result) {
  if (!result.length) {
    console.log('Nothing to migrate')
  }
  console.log(`\n${chalk.black.bgGreen('DONE')}`)
}
