CREATE TABLE `service_order_document` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `service_order_id` int(11) DEFAULT NULL,
  `document_id` int(11) DEFAULT NULL,
  `user_id` int(11) DEFAULT NULL,
  `provider_id` int(11) DEFAULT NULL,
  `created_at` datetime NOT NULL,
  `updated_at` datetime DEFAULT NULL,
  `enabled` tinyint(1) NOT NULL DEFAULT '1',
  `deleted` tinyint(1) NOT NULL DEFAULT '0',
  PRIMARY KEY (`id`),
  KEY `IDX_USER_TABLE_SERVICE_ORDER_DOCUMENT` (`user_id`),
  KEY `IDX_PROVIDER_TABLE_SERVICE_ORDER_DOCUMENT` (`provider_id`),
  KEY `IDX_SERVICE_ORDER_TABLE_SERVICE_ORDER_DOCUMENT` (`service_order_id`),
  KEY `IDX_DOCUMENT_TABLE_SERVICE_ORDER_DOCUMENT` (`document_id`),
  CONSTRAINT `FK_SERVICE_ORDER_TABLE_SERVICE_ORDER_DOCUMENT` FOREIGN KEY (`service_order_id`) REFERENCES `service_order` (`id`),
  CONSTRAINT `FK_DOCUMENT_TABLE_SERVICE_ORDER_DOCUMENT` FOREIGN KEY (`document_id`) REFERENCES `document` (`id`),
  CONSTRAINT `FK_PROVIDER_TABLE_SERVICE_ORDER_DOCUMENT` FOREIGN KEY (`provider_id`) REFERENCES `provider` (`id`),
  CONSTRAINT `FK_USER_TABLE_SERVICE_ORDER_DOCUMENT` FOREIGN KEY (`user_id`) REFERENCES `user` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=13 DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci
