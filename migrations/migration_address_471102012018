CREATE TABLE `btimeapp_prod`.`address` (
  `id` INT NOT NULL AUTO_INCREMENT,
  `search` TEXT NULL DEFAULT NULL,
  `postcode` VARCHAR(10) NOT NULL,
  `address` VARCHAR(120) NULL DEFAULT NULL,
  `number` VARCHAR(45) NULL DEFAULT NULL,
  `complement` VARCHAR(100) NULL DEFAULT NULL,
  `neighborhood` VARCHAR(155) NULL DEFAULT NULL,
  `city` VARCHAR(120) NULL DEFAULT NULL,
  `state` VARCHAR(60) NULL DEFAULT NULL,
  `country` VARCHAR(60) NULL DEFAULT NULL,
  `created_at` DATETIME NULL DEFAULT NULL,
  `updated_at` DATETIME NULL DEFAULT NULL,
  `enabled` TINYINT(1) NOT NULL DEFAULT '1',
  `deleted` TINYINT(1) NOT NULL DEFAULT '0',
  PRIMARY KEY (`id`));